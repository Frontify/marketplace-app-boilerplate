/* DO NOT DELETE:
This file can be modified but must remain within this repository 
for code checks performed during the review process. */
{
    "compilerOptions": {
        "esModuleInterop": true,
        "isolatedModules": true,
        "module": "commonjs",
        "moduleResolution": "node",
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "noImplicitThis": true,
        "sourceMap": true,
        "strict": true,
        "target": "es6",
        "jsx": "react-jsx",
        "lib": ["ES2021", "DOM"],
        "resolveJsonModule": true,
        "skipLibCheck": true
    },
    "exclude": ["dist", "node_modules"]
}
